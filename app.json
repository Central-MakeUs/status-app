{
  "expo": {
    "name": "status-app",
    "slug": "status-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "statusapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stackoverflow.status",
      "associatedDomains": ["applinks:status-front-rho.vercel.app"]
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.stackoverflow.status"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
